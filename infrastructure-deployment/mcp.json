{
  "mcpServers": {
    "cdk": {
      "command": "uvx",
      "args": ["awslabs.cdk-mcp-server@latest"],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR",
        "AWS_PROFILE": "${AWS_PROFILE}"
      },
      "disabled": false,
      "autoApprove": []
    },
    "cloudformation": {
      "command": "uvx",
      "args": ["awslabs.cfn-mcp-server@latest"],
      "env": {
        "AWS_PROFILE": "${AWS_PROFILE}"
      },
      "disabled": false,
      "autoApprove": []
    },
    "terraform": {
      "command": "uvx",
      "args": ["awslabs.terraform-mcp-server@latest"],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR",
        "AWS_PROFILE": "${AWS_PROFILE}"
      },
      "disabled": false,
      "autoApprove": []
    },
    "lambda": {
      "command": "uvx",
      "args": ["awslabs.lambda-tool-mcp-server@latest"],
      "env": {
        "AWS_PROFILE": "${AWS_PROFILE}",
        "AWS_REGION": "${AWS_REGION}",
        "FUNCTION_PREFIX": "${FUNCTION_PREFIX}",
        "FUNCTION_LIST": "${FUNCTION_LIST}",
        "FUNCTION_TAG_KEY": "${FUNCTION_TAG_KEY}",
        "FUNCTION_TAG_VALUE": "${FUNCTION_TAG_VALUE}"
      },
      "disabled": true,
      "autoApprove": []
    },
    "serverless": {
      "command": "uvx",
      "args": [
        "awslabs.aws-serverless-mcp-server@latest",
        "--allow-write",
        "--allow-sensitive-data-access"
      ],
      "env": {
        "AWS_PROFILE": "${AWS_PROFILE}",
        "AWS_REGION": "${AWS_REGION}"
      },
      "disabled": true,
      "autoApprove": []
    },
    "ecs": {
      "command": "uvx",
      "args": ["--from", "awslabs-ecs-mcp-server", "ecs-mcp-server"],
      "env": {
        "AWS_PROFILE": "${AWS_PROFILE}",
        "AWS_REGION": "${AWS_REGION}",
        "FASTMCP_LOG_LEVEL": "ERROR",
        "ALLOW_WRITE": "false",
        "ALLOW_SENSITIVE_DATA": "false"
      },
      "disabled": true,
      "autoApprove": []
    },
    "eks": {
      "command": "uvx",
      "args": [
        "awslabs.eks-mcp-server@latest",
        "--allow-write",
        "--allow-sensitive-data-access"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR",
        "AWS_PROFILE": "${AWS_PROFILE}"
      },
      "transportType": "stdio",
      "disabled": true,
      "autoApprove": []
    },
    "finch": {
      "command": "uvx",
      "args": ["awslabs.finch-mcp-server@latest"],
      "env": {
        "AWS_PROFILE": "${AWS_PROFILE}",
        "AWS_REGION": "${AWS_REGION}",
        "FASTMCP_LOG_LEVEL": "INFO"
      },
      "transportType": "stdio",
      "disabled": true,
      "autoApprove": []
    },
    "ccapi": {
      "command": "uvx",
      "args": ["awslabs.ccapi-mcp-server@latest"],
      "env": {
        "AWS_PROFILE": "${AWS_PROFILE}",
        "DEFAULT_TAGS": "enabled",
        "SECURITY_SCANNING": "enabled",
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": true,
      "autoApprove": []
    },
    "support": {
      "command": "uvx",
      "args": [
        "-m",
        "awslabs.aws-support-mcp-server@latest",
        "--debug",
        "--log-file",
        "./logs/mcp_support_server.log"
      ],
      "env": {
        "AWS_PROFILE": "${AWS_PROFILE}"
      },
      "disabled": true,
      "autoApprove": []
    }
  },
  "disabled": false
}
